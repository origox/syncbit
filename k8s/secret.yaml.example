apiVersion: v1
kind: Secret
metadata:
  name: syncbit-secrets
  namespace: default
type: Opaque
stringData:
  # Fitbit OAuth2 credentials
  # Get these from https://dev.fitbit.com/apps
  FITBIT_CLIENT_ID: "your_client_id_here"
  FITBIT_CLIENT_SECRET: "your_client_secret_here"
  
  # Victoria Metrics credentials
  VICTORIA_USER: "your_username_here"
  VICTORIA_PASSWORD: "your_password_here"
